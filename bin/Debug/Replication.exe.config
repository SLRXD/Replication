<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<!--发布数据库和订阅数据库的数据库名称、登录名和密码须一致-->
		<!--数据库名称-->
		<add key="Subscriber_db" value="Dome"/>
		<!--数据库登录名-->
		<add key="Subscriber_login" value="sa"/>
		<!--数据库密码-->
		<add key="Subscriber_password" value="Ld123456"/>
		<!--订阅服务器实例名-->
		<add key="Subscriber" value="LAPTOP-1MF3PKAK"/>
		<!--发布服务器实例名-->
		<add key="Publisher" value="LAPTOP-C0CIJ5TH"/>
		<!--订阅数据库作业-->
		<add key="Subscriber_job" value="update"/>
	</appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
</configuration>
